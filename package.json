		"chalk": "^2.4.1",
		"fancy-log": "^1.3.2",
		"gulp": "^3.9.1",
		"gulp-autoprefixer": "^5.0.0",
		"gulp-concat": "^2.6.1",
		"gulp-cssnano": "^2.1.3",
		"gulp-htmlmin": "^4.0.0",
		"gulp-livereload": "^3.8.1",
		"gulp-newer": "^1.4.0",
		"gulp-notify": "^3.2.0",
		"gulp-plumber": "^1.2.0",
		"gulp-rename": "^1.2.2",
		"gulp-sass": "^4.0.1",
		"gulp-size": "^3.0.0",
